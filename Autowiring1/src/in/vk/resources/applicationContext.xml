<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="in.vk.beans.Address" id="address">
		<property name="house" value="521" />
		<property name="city" value="Gurgaon" />
		<property name="pin" value="122001" />

	</bean>

	<bean class="in.vk.beans.Student" id="stdObj" autowire="byType">
		<property name="name" value="Vineet Kundu"></property>
		<property name="rollno" value="102"></property>
	</bean>

</beans>